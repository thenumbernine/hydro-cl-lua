	static inline void eigenWaveCodeMinMax(
		constant Solver const & solver,
		Eigen const & eig,
		Normal const n,
		real3 const pt,
		real * resultMin,
		real * resultMax
	) {
		auto calcWaves = EigenWaveCode(solver, eig, n, pt);
