		constexpr real operator()(
			constant Solver const & solver,
			Cons const & U,
			Normal const n,
			real3 const pt,
			int waveIndex
		) const {
			if (waveIndex == 0) {
				return v_n - Cs_nLen;
			} else if (waveIndex >= 1 && waveIndex <= 3) {
				return v_n;
			} else if (waveIndex == 4) {
				return v_n + Cs_nLen;
			}
			return {};
		}
	};

