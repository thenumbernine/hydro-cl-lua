// the following are 3D specific:

template<
	typename A,
	typename B
>
constexpr auto cross(
	A const & a,
	B const & b,
	std::enable_if_t<IsBinaryTensorR3xR3Op<A,B>>*
) {
	using RS = decltype(typename A::Scalar() * typename B::Scalar());
	using R = typename A::template ReplaceScalar<RS>;
	return R(
		a[1] * b[2] - a[2] * b[1],
		a[2] * b[0] - a[0] * b[2],
		a[0] * b[1] - a[1] * b[0]);
}

