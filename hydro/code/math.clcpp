#define TENSOR_ADD_RANK_IMPL()\
	struct RankImpl {\
		static constexpr int value() {\
			if constexpr (is_tensor_v<Inner>) {\
				return Inner::RankImpl::value() + localRank;\
			} else {\
				return localRank;\
			}\
		}\
	};\
