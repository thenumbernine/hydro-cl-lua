struct RotateFromInDimImpl<1, T, N> {
	static constexpr auto value(
		T const & v,
		N const & n,
		std::enable_if_t<is_tensor_v<T> && is_tensor_v<N> && N::rank == 1 && N::template dim<0> == 3>* = {}
